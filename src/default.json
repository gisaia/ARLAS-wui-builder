        "unmanagedFields": {
          "renderStep": {
            "showExportCsv": true,
            "isHistogramSelectable": true,
            "topOffsetRemoveInterval": 40,
            "leftOffsetRemoveInterval": 18,
            "brushHandlesHeightWeight": 0.8,
            "yAxisStartsFromZero": true,
            "xAxisPosition": "bottom",
            "descriptionPosition": "bottom",
            "xTicks": 4,
            "yTicks": 1,
            "xLabels": 4,
            "yLabels": 4,
            "showXTicks": true,
            "showYTicks": true,
            "showXLabels": true,
            "showYLabels": true,
            "xUnit": "",
            "yUnit": "",
            "chartXLabel": "",
            "shortYLabels": false,
            "barWeight": 0.8,
            "swimLaneLabelsWidth": 100,
            "swimlaneHeight": 20,
            "swimlaneBorderRadius": 3
          }
        }
      },
      "metric": {
        "renderStep": {
          "valuePrecision": 1
        },
        "unmanagedFields": {
          "renderStep": {
            "customizedCssClass": "power"
          }
        }
      },
      "donut": {
        "unmanagedFields": {
          "renderStep": {
            "customizedCssClass": "arlas-donuts-analytics",
            "diameter": 150
          }
        },
        "renderStep": {
          "opacity": 0.4,
          "multiselectable": true
        }
      },
      "powerbar": {
        "dataStep": {
          "aggregationSize": 10,
          "operator": "Eq",
          "metric": {
            "metricCollectFunction": "Count"
          }
        },
        "renderStep": {
          "scrollable": true,
          "allowOperatorChange": true,
          "showExportCsv": true,
          "displayFilter": true
        }
      },
      "resultlist": {
        "dataStep": {
          "searchSize": 100
        },
        "renderStep": {
          "cellBackgroundStyle": "filled"
        },
        "unmanagedFields": {
          "dataStep": {},
