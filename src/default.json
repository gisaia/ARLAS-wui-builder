{
    "global": {
        "serverUrl": "http://localhost:19999/arlas",
        "unmanagedFields": {
            "maxAgeCache": 120,
            "appName": "ARLAS",
            "appUnit": "",
            "appNameBackgroundColor": "#FF4081"
        }
    },
    "map": {
        "global": {
            "geographicalOperator": "intersects",
            "margePanForLoad": 5,
            "margePanForTest": 5,
            "initZoom": 2,
            "allowMapExtend": true,
            "initCenterLat": 0,
            "initCenterLon": 0,
            "displayScale": true,
            "displayCurrentCoordinates": false,
            "unmanagedFields": {
                "icon": "check_box_outline_blank",
                "nbVerticesLimit": 100,
                "mapLayers": {
                    "events": {
                        "zoomOnClick": [],
                        "emitOnClick": [],
                        "onHover": []
                    }
                }
            }
        },
        "basemap": {
            "default": "Positron"
        },
        "layer": {
            "featuresFg": {
                "geometryStep": {
                    "geometry": "",
                    "granularity": "Coarse"
                },
                "visibilityStep": {
                    "visible": true,
                    "zoomMin": 1,
                    "zoomMax": 22,
                    "featuresMax": 5000,
                    "featuresMin": 5000
                },
                "styleStep": {
                    "geometryType": "fill",
                    "lineType": "solid",
                    "opacity": {
                        "propertySource": "Fix",
                        "propertyFix": 1
                    },
                    "colorFg": {
                        "propertySource": "Fix",
                        "propertyFix": "#abcdef"
                    },
                    "radiusFg": {
                        "propertySource": "Fix",
                        "propertyFix": 1.5
                    },
                    "strokeColorFg": {
                        "propertySource": "Fix",
                        "propertyFix": "#abcdef"
                    },
                    "strokeWidthFg": {
                        "propertySource": "Fix",
                        "propertyFix": 1
                    },
                    "strokeOpacityFg": {
                        "propertySource": "Fix",
                        "propertyFix": 1
                    },
                    "widthFg": {
                        "propertySource": "Fix",
                        "propertyFix": 2
                    }
                }
            },
            "featureMetricFg": {
                "geometryStep": {
                    "geometry": "",
                    "granularity": "Medium"
                },
                "visibilityStep": {
                    "visible": true,
                    "zoomMin": 1,
                    "zoomMax": 22,
                    "featuresMax": 5000
                },
                "styleStep": {
                    "geometryType": "fill",
                    "lineType": "solid",
                    "opacity": {
                        "propertySource": "Fix",
                        "propertyFix": 1
                    },
                    "colorFg": {
                        "propertySource": "Fix",
                        "propertyFix": "#abcdef"
                    },
                    "radiusFg": {
                        "propertySource": "Fix",
                        "propertyFix": 1.5
                    },
                    "strokeColorFg": {
                        "propertySource": "Fix",
                        "propertyFix": "#abcdef"
                    },
                    "strokeWidthFg": {
                        "propertySource": "Fix",
                        "propertyFix": 1
                    },
                    "strokeOpacityFg": {
                        "propertySource": "Fix",
                        "propertyFix": 1
                    },
                    "widthFg": {
                        "propertySource": "Fix",
                        "propertyFix": 2
                    }
                }
            },
            "clusterFg": {
                "geometryStep": {
                    "geometry": "",
                    "granularity": "Medium",
                    "clusterGeometryType": "aggregated_geometry",
                    "aggregatedGeometry": "cell",
                    "aggType": "tile"
                },
                "visibilityStep": {
                    "visible": true,
                    "zoomMin": 1,
                    "zoomMax": 22,
                    "featuresMin": 5000
                },
                "styleStep": {
                    "geometryType": "fill",
                    "lineType": "solid",
                    "opacity": {
                        "propertySource": "Fix",
                        "propertyFix": 0.7
                    },
                    "colorFg": {
                        "propertySource": "Fix",
                        "propertyFix": "#abcdef"
                    },
                    "radiusFg": {
                        "propertySource": "Fix",
                        "propertyFix": 1.5
                    },
                    "strokeColorFg": {
                        "propertySource": "Fix",
                        "propertyFix": "#abcdef"
                    },
                    "strokeWidthFg": {
                        "propertySource": "Fix",
                        "propertyFix": 1
                    },
                    "strokeOpacityFg": {
                        "propertySource": "Fix",
                        "propertyFix": 1
                    },
                    "weightFg": {
                        "propertySource": "Fix",
                        "propertyFix": 1
                    },
                    "intensityFg": {
                        "propertySource": "Fix",
                        "propertyFix": 1
                    }
                }
            }
        }
    },
    "search": {
        "global": {
            "name": "Search...",
            "searchField": "internal.fulltext",
            "autocompleteField": "internal.fulltext",
            "autocompleteSize": 10,
            "unmanagedFields": {
                "icon": "search"
            }
        }
    },
    "lookAndFeel": {
        "global": {
            "dragAndDrop": true,
            "indicators": true,
            "spinner": true,
            "spinnerColor": "accent",
            "spinnerDiameter": 50,
            "zoomToData": true
        }
    },
    "timeline": {
        "global": {
            "useDetailedTimeline": true,
            "useUtc": true,
            "tabsContainer": {
                "dataStep": {
                    "timeline": {
                        "aggregation": {
                            "aggregationBucketOrInterval": "bucket",
                            "aggregationBucketsNumber": 50
                        }
                    },
                    "detailedTimeline": {
                        "bucketsNumber": 50
                    }
                },
                "renderStep": {
                    "timeline": {
                        "chartTitle": "Timeline",
                        "chartType": "bars",
                        "dateFormat": "%b %d %Y  %H:%M",
                        "isMultiselectable": true
                    },
                    "detailedTimeline": {
                        "chartTitle": "Detailed timeline",
                        "chartType": "bars",
                        "dateFormat": "%b %d %Y  %H:%M",
                        "selectionExtentPercent": 2
                    }
                }
            },
            "unmanagedFields": {
                "dataStep": {
                    "timeline": {
                        "name": "Timeline",
                        "icon": "watch_later",
                        "isOneDimension": false
                    },
                    "detailedTimeline": {
                        "name": "Timeline",
                        "icon": "watch_later",
                        "isOneDimension": false
                    }
                },
                "renderStep": {
                    "timeline": {
                        "xTicks": 9,
                        "yTicks": 2,
                        "xLabels": 9,
                        "yLabels": 2,
                        "xUnit": "",
                        "yUnit": "",
                        "chartXLabel": "",
                        "shortYLabels": false,
                        "customizedCssClass": "arlas-timeline",
                        "chartHeight": 128,
                        "brushHandlesHeightWeight": 0.8,
                        "isHistogramSelectable": true,
                        "chartWidth": null,
                        "xAxisPosition": "bottom",
                        "yAxisStartsFromZero": true,
                        "descriptionPosition": "top",
                        "showXTicks": true,
                        "showYTicks": true,
                        "showXLabels": true,
                        "showYLabels": true,
                        "showHorizontalLines": false,
                        "isSmoothedCurve": true,
                        "barWeight": 0.8,
                        "topOffsetRemoveInterval": 10
                    },
                    "detailedTimeline": {
                        "xTicks": 5,
                        "yTicks": 2,
                        "xLabels": 5,
                        "yLabels": 2,
                        "xUnit": "",
                        "yUnit": "",
                        "chartXLabel": "",
                        "shortYLabels": false,
                        "customizedCssClass": "arlas-detailed-timeline",
                        "chartHeight": 60,
                        "multiselectable": false,
                        "brushHandlesHeightWeight": 0.8,
                        "isHistogramSelectable": true,
                        "chartWidth": null,
                        "xAxisPosition": "top",
                        "yAxisStartsFromZero": true,
                        "descriptionPosition": "bottom",
                        "showXTicks": true,
                        "showYTicks": true,
                        "showXLabels": true,
                        "showYLabels": true,
                        "showHorizontalLines": false,
                        "isSmoothedCurve": true,
                        "barWeight": 0.8
                    }
                }
            }
        }
    },
    "analytics": {
        "tabs": {
            "default": "Default",
            "new": "New tab"
        },
        "groups": {
            "new": "New group"
        },
        "widgets": {
            "histogram": {
                "dataStep": {
                    "aggregation": {
                        "aggregationBucketOrInterval": "bucket",
                        "aggregationBucketsNumber": 50
                    },
                    "useUtc":true,
                    "metric" : {
                        "metricCollectFunction": "Count"
                    }
                },
                "renderStep": {
                    "multiselectable": true,
                    "chartType": "bars",
                    "showHorizontalLines": true,
                    "ticksDateFormat": "%b %d %Y  %H:%M"
                },
                "unmanagedFields": {
                    "dataStep": {
                        "isOneDimension": false
                    },
                    "renderStep": {
                        "showExportCsv": true,
                        "isHistogramSelectable": true,
                        "topOffsetRemoveInterval": 40,
                        "leftOffsetRemoveInterval": 18,
                        "brushHandlesHeightWeight": 0.8,
                        "yAxisStartsFromZero": true,
                        "xAxisPosition": "bottom",
                        "descriptionPosition": "bottom",
                        "xTicks": 4,
                        "yTicks": 1,
                        "xLabels": 4,
                        "yLabels": 4,
                        "showXTicks": true,
                        "showYTicks": true,
                        "showXLabels": true,
                        "showYLabels": true,
                        "xUnit": "",
                        "yUnit": "",
                        "chartXLabel": "",
                        "shortYLabels": false,
                        "barWeight": 0.8,
                        "isSmoothedCurve": false
                    }
                }
            },
            "swimlane": {
                "dataStep": {
                    "termAggregation": {
                        "termAggregationSize": 5
                    },
                    "aggregation": {
                        "aggregationBucketsNumber": 50
                    },
                    "metric": {
                        "metricCollectFunction": "Count"
                    },
                    "useUtc":true
                },
                "renderStep": {
                    "swimlaneMode": "fixedHeight",
                    "paletteColors": [
                        330,
                        170
                    ]
                },
                "unmanagedFields": {
                    "renderStep": {
                        "showExportCsv": true,
                        "isHistogramSelectable": true,
                        "topOffsetRemoveInterval": 40,
                        "leftOffsetRemoveInterval": 18,
                        "brushHandlesHeightWeight": 0.8,
                        "yAxisStartsFromZero": true,
                        "xAxisPosition": "bottom",
                        "descriptionPosition": "bottom",
                        "xTicks": 4,
                        "yTicks": 1,
                        "xLabels": 4,
                        "yLabels": 4,
                        "showXTicks": true,
                        "showYTicks": true,
                        "showXLabels": true,
                        "showYLabels": true,
                        "xUnit": "",
                        "yUnit": "",
                        "chartXLabel": "",
                        "shortYLabels": false,
                        "barWeight": 0.8,
                        "swimLaneLabelsWidth": 100,
                        "swimlaneHeight": 20,
                        "swimlaneBorderRadius": 3
                    }
                }
            },
            "metric": {
                "renderStep": {
                    "valuePrecision": 1
                },
                "unmanagedFields": {
                    "renderStep": {
                        "customizedCssClass": "power"
                    }
                }
            },
            "donut": {
                "unmanagedFields": {
                    "renderStep": {
                        "customizedCssClass": "arlas-donuts-analytics",
                        "diameter": 150
                    }
                },
                "renderStep": {
                    "opacity": 0.4,
                    "multiselectable": true
                }
            },
            "powerbar": {
                "dataStep": {
                    "aggregationSize": 10,
                    "metric" : {
                        "metricCollectFunction": "Count"
                    }
                }
            },
            "resultlist": {
                "dataStep": {
                    "searchSize": 100
                },
                "renderStep": {
                    "cellBackgroundStyle": "filled"
                },
                "unmanagedFields": {
                    "dataStep": {},
                    "renderStep": {
                        "tableWidth": 445,
                        "globalActionsList": [],
                        "nLastLines": 3,
                        "detailedGridHeight": 25,
                        "nbGridColumns": 3,
                        "defautMode": "list",
                        "isBodyHidden": false,
                        "isGeoSortActived": false,
                        "isAutoGeoSortActived": true,
                        "selectedItemsEvent": null,
                        "consultedItemEvent": null,
                        "actionOnItemEvent": null,
                        "globalActionEvent": null
                    }
                }
            }
        }
    },
    "sideModules": {
        "global": {
            "useShare": true,
            "useDownload": true,
            "useTagger": false,
            "share": {
                "maxForFeature": 5000,
                "maxForTopology": 5000
            },
            "tagger": {
                "serverUrl": "http://tagger.com",
                "collection": "demo_ais-danmark"
            },
            "unmanagedFields": {
                "sortExcludedTypes": [
                    "TEXT",
                    "GEO_POINT"
                ],
                "download": {
                    "authType": "basic"
                }
            }
        }
    },
