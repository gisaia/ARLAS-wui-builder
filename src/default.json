{
    "global": {
        "serverUrl": "http://demo.arlas.io/arlas",
        "collections": ["demo_ais-danmark"]
    },
    "map": {
        "layer": {
            "visibilityStep": {
                "zoomMinCtrl": 10,
                "zoomMaxCtrl": 10,
                "featuresMaxCtrl": 7500
            },
            "styleStep": {
                "opacityCtrl": 0.5,
                "colorSourceCtrl": "manual",
                "choosenColorGrp": {
                    "colorFixCtrl": "#abcdef",
                    "colorInterpolatedGroup": {
                        "colorInterpolatedNormalizeCtrl": true,
                        "colorInterpolatedMinValueCtrl": 5
                    }
                }
            }
        }
    },
    "config": {
        "aggregationTermSize": 20,
        "palettes": [["#065143", "#ce1483"], ["#6d213c", "#946846", "#baab68"], ["#eecf6d", "#d5ac4e", "#8b6220", "#720e07", "#45050c"]],
        "colorPickerPresets": ["#fff", "#000", "#2889e9", "#e920e9", "#fff500", "rgb(236,64,64)"],
        "otherColor": "#9d9ca9"
    }
}