{
    "global": {
        "serverUrl": "http://demo.arlas.io/arlas",
        "collections": ["demo_ais-danmark"]
    },
    "map": {
        "global": {
            "geographicalOperator": "intersects"
        },
        "layer": {
            "collectionStep": {
                "collectionCtrl": "demo_ais-danmark"
            },
            "geometryStep": {
                "geometryCtrl": "position.location",
                "geometryTypeCtrl": "circle",
                "granularityCtrl": "coarse"
            },
            "visibilityStep": {
                "zoomMinCtrl": 10,
                "zoomMaxCtrl": 10,
                "featuresMaxCtrl": 7500,
                "featuresMinCtrl": 7500
            },
            "styleStep": {
                "opacityCtrl": 0.5,
                "colorFg": {
                    "propertySourceCtrl": "Fix",
                    "propertyFixCtrl": "#abcdef"
                },
                "radiusFg": {
                    "propertySourceCtrl": "Fix",
                    "propertyFixCtrl": "1.5"
                }
            }
        }
    },
    "search": {
        "global": {
            "autocompleteSize": 10,
            "name": "Search a ...",
            "searchField": "id",
            "autocompleteField": "id",
            "icon": "search"
        }
    },
    "timeline": {
        "global": {
            "useDetailedTimeline": true,
            "timeline": {
                "field": "position.timestamp",
                "bucketsNumber": 22,
                "intervalSize": 10,
                "chartTitle": "title",
                "chartType": "area"
            },
            "detailedTimeline": {
                "field": "position.timestamp",
                "bucketsNumber": 44,
                "intervalSize": 10,
                "chartTitle": "title",
                "chartType": "area",
                "selectionExtentPercent": 2
            }
        }
    },
    "config": {
        "aggregationTermSize": 20,
        "palettes": [["#065143", "#ce1483"], ["#6d213c", "#946846", "#baab68"], ["#eecf6d", "#d5ac4e", "#8b6220", "#720e07", "#45050c"]],
        "colorPickerPresets": ["#fff", "#000", "#2889e9", "#e920e9", "#fff500", "rgb(236,64,64)"],
        "otherColor": "#9d9ca9",
        "widthMin": 0,
        "widthMax": 15,
        "widthStep": 0.5,
        "radiusMin": 1,
        "radiusMax": 3,
        "radiusStep": 0.1
    }
}