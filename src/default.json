{
    "global": {
        "serverUrl": "${ARLAS_SERVER_URL}",
        "unmanagedFields": {
            "maxAgeCache": 120
        }        
    },
    "map": {
        "global": {
            "geographicalOperator": "intersects",
            "margePanForLoad": 5,
            "margePanForTest": 5,
            "initZoom": 2,
            "allowMapExtend": true,
            "initCenterLat": 0,
            "initCenterLon": 0,
            "displayScale": true,
            "unmanagedFields": {
                "icon": "check_box_outline_blank",
                "nbVerticesLimit": 100,
                "defaultBasemapStyle": {
                    "name": "Positron",
                    "styleFile": "https://api.maptiler.com/maps/positron/style.json?key=xIhbu1RwgdbxfZNmoXn4"
                },
                "basemapStyles": [
                    {
                        "name": "Positron",
                        "styleFile": "https://api.maptiler.com/maps/positron/style.json?key=xIhbu1RwgdbxfZNmoXn4"
                    }
                ],
                "mapLayers": {
                  "events": {
                    "zoomOnClick": [],
                    "emitOnClick": [],
                    "onHover": []
                  }
                }
              }
        },
        "layer": {
            "featuresFg": {
                "geometryStep": {
                    "geometry": "position.location",
                    "granularity": "Coarse"
                },
                "visibilityStep": {
                    "visible": true,
                    "zoomMin": 0,
                    "zoomMax": 22,
                    "featuresMax": 5000
                },
                "styleStep": {
                    "geometryType": "fill",
                    "opacity": 0.5,
                    "colorFg": {
                        "propertySource": "Fix",
                        "propertyFix": "#abcdef"
                    },
                    "radiusFg": {
                        "propertySource": "Fix",
                        "propertyFix": 1.5
                    }
                }
            },
            "featureMetricFg": {
                "geometryStep": {
                    "geometry": "position.location",
                    "granularity": "Coarse"
                },
                "visibilityStep": {
                    "visible": true,
                    "zoomMin": 0,
                    "zoomMax": 22,
                    "featuresMax": 5000
                },
                "styleStep": {
                    "geometryType": "fill",
                    "opacity": 0.5,
                    "colorFg": {
                        "propertySource": "Fix",
                        "propertyFix": "#abcdef"
                    },
                    "radiusFg": {
                        "propertySource": "Fix",
                        "propertyFix": 1.5
                    }
                }
            },
            "clusterFg": {
                "geometryStep": {
                    "geometry": "position.location",
                    "granularity": "Coarse"
                },
                "visibilityStep": {
                    "visible": true,
                    "zoomMin": 0,
                    "zoomMax": 22,
                    "featuresMin": 5000
                },
                "styleStep": {
                    "geometryType": "fill",
                    "opacity": 0.5,
                    "colorFg": {
                        "propertyFix": "#abcdef"
                    },
                    "radiusFg": {
                        "propertySource": "Fix",
                        "propertyFix": 1.5
                    }
                }
            }
        }
    },
    "search": {
        "global": {
            "searchField": "internal.fulltext",
            "autocompleteField": "internal.fulltext",
            "unmanagedFields": {
                "icon": "search"
            }
        }
    },
    "lookAndFeel": {
        "global": {
            "dragAndDrop": true
        }
    },
    "timeline": {
        "global": {
            "useDetailedTimeline": false,
            "tabsContainer": {
                "dataStep": {
                    "timeline": {
                        "aggregation": {
                            "aggregationBucketOrInterval": "bucket",
                            "aggregationBucketsNumber": 50
                        }
                    },
                    "detailedTimeline": {
                        "bucketsNumber": 50
                    }
                },
                "renderStep": {
                    "timeline": {
                        "chartTitle": "title",
                        "chartType": "area",
                        "dateFormat": "%b %d %Y  %H:%M"
                    },
                    "detailedTimeline": {
                        "chartTitle": "detailed title",
                        "chartType": "area",
                        "dateFormat": "%b %d %Y  %H:%M",
                        "selectionExtentPercent": 2
                    }
                }
            },
            "unmanagedFields": {
                "dataStep": {
                    "timeline": {
                      "name": "Timeline",
                      "icon": "watch_later",
                      "isOneDimension": false
                    },
                    "detailedTimeline": {
                      "name": "Timeline",
                      "icon": "watch_later",
                      "isOneDimension": false
                    }
                  },
                  "renderStep": {
                    "timeline": {
                      "xTicks": 9,
                      "yTicks": 2,
                      "xLabels": 9,
                      "yLabels": 2,
                      "xUnit": "",
                      "yUnit": "",
                      "chartXLabel": "",
                      "shortYLabels": false,
                      "customizedCssClass": "arlas-timeline",
                      "chartHeight": 128,
                      "brushHandlesHeightWeight": 0.8,
                      "isHistogramSelectable": true,
                      "chartWidth": null,
                      "xAxisPosition": "bottom",
                      "yAxisStartsFromZero": true,
                      "descriptionPosition": "top",
                      "showXTicks": true,
                      "showYTicks": true,
                      "showXLabels": true,
                      "showYLabels": true,
                      "showHorizontalLines": false,
                      "isSmoothedCurve": true,
                      "barWeight": 0.8,
                      "topOffsetRemoveInterval": 10
                    },
                    "detailedTimeline": {
                      "xTicks": 5,
                      "yTicks": 2,
                      "xLabels": 5,
                      "yLabels": 2,
                      "xUnit": "",
                      "yUnit": "",
                      "chartXLabel": "",
                      "shortYLabels": false,
                      "customizedCssClass": "arlas-detailed-timeline",
                      "chartHeight": 60,
                      "multiselectable": false,
                      "brushHandlesHeightWeight": 0.8,
                      "isHistogramSelectable": true,
                      "chartWidth": null,
                      "xAxisPosition": "top",
                      "yAxisStartsFromZero": true,
                      "descriptionPosition": "bottom",
                      "showXTicks": true,
                      "showYTicks": true,
                      "showXLabels": true,
                      "showYLabels": true,
                      "showHorizontalLines": false,
                      "isSmoothedCurve": true,
                      "barWeight": 0.8
                    }
                  }
            }
        }
    },
    "analytics": {
        "tabs": {
            "default": "Default",
            "new": "New tab"
        },
        "groups": {
            "new": "New group"
        },
        "widgets": {
            "histogram": {
                "dataStep": {
                    "bucketOrInterval": true,
                    "bucketsNumber": 5,
                    "aggregationValue": "COUNT"
                },
                "renderStep": {
                    "multiselectable": true,
                    "chartType": "bars",
                    "showHorizontalLines": true,
                    "ticksDateFormat": "%b %d %Y  %H:%M"
                },
                "unmanagedFields": {
                    "dataStep": {
                      "isOneDimension": false
                    },
                    "renderStep": {
                      "showExportCsv": true,
                      "isHistogramSelectable": true,
                      "topOffsetRemoveInterval": 40,
                      "leftOffsetRemoveInterval": 18,
                      "brushHandlesHeightWeight": 0.8,
                      "yAxisStartsFromZero": true,
                      "xAxisPosition": "bottom",
                      "descriptionPosition": "bottom",
                      "xTicks": 4,
                      "yTicks": 1,
                      "xLabels": 4,
                      "yLabels": 4,
                      "showXTicks": true,
                      "showYTicks": true,
                      "showXLabels": true,
                      "showYLabels": true,
                      "xUnit": "",
                      "yUnit": "",
                      "chartXLabel": "",
                      "shortYLabels": false,
                      "barWeight": 0.8,
                      "isSmoothedCurve": false
                    }
                  }
            },
            "swimlane": {
                "dataStep": {
                    "termAggregation": {
                        "termAggregationSize": 5
                    }
                },
                "unmanagedFields": {
                    "renderStep": {
                      "showExportCsv": true,
                      "isHistogramSelectable": true,
                      "topOffsetRemoveInterval": 40,
                      "leftOffsetRemoveInterval": 18,
                      "brushHandlesHeightWeight": 0.8,
                      "yAxisStartsFromZero": true,
                      "xAxisPosition": "bottom",
                      "descriptionPosition": "bottom",
                      "xTicks": 4,
                      "yTicks": 1,
                      "xLabels": 4,
                      "yLabels": 4,
                      "showXTicks": true,
                      "showYTicks": true,
                      "showXLabels": true,
                      "showYLabels": true,
                      "xUnit": "",
                      "yUnit": "",
                      "chartXLabel": "",
                      "shortYLabels": false,
                      "barWeight": 0.8,
                      "swimLaneLabelsWidth": 100,
                      "swimlaneHeight": 20,
                      "swimlaneBorderRadius": 3
                    }
                }
            },
            "metric": {
                "unmanagedFields": {
                    "renderStep": {
                        "customizedCssClass": "power"
                    }
                }
            },
            "donut": {
                "unmanagedFields": {
                    "renderStep": {
                      "customizedCssClass": "arlas-donuts-analytics",
                      "diameter": 150
                    }
                  }
            },
            "resultlist"   : {
                "unmanagedFields": {
                    "dataStep": {},
                    "renderStep": {
                      "tableWidth": 455,
                      "globalActionsList": [],
                      "nLastLines": 3,
                      "detailedGridHeight": 25,
                      "nbGridColumns": 3,
                      "defautMode": "list",
                      "isBodyHidden": false,
                      "isGeoSortActived": false,
                      "isAutoGeoSortActived": true,
                      "selectedItemsEvent": null,
                      "consultedItemEvent": null,
                      "actionOnItemEvent": null,
                      "globalActionEvent": null
                    }
                }
            }
        }
    },
    "sideModules": {
        "global": {
            "useShare": true,
            "useDownload": true,
            "useTagger": true,
            "share": {
                "maxForFeature": 1234,
                "maxForTopology": 5678
            },
            "tagger": {
                "serverUrl": "http://tagger.com",
                "collection": "demo_ais-danmark"
            },
            "unmanagedFields": {
                "sortExcludedTypes": [
                    "TEXT",
                     "GEO_POINT"
                 ],
                 "download": {
                    "authType": "basic"
                 }
            }
        }
    },
