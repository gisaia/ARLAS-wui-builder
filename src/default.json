  "search": {
    "global": {
      "name": "Search...",
      "searchField": "internal.fulltext",
      "autocompleteField": "internal.autocomplete",
      "autocompleteSize": 10,
      "unmanagedFields": {
        "icon": "search"
      }
    }
  },
  "lookAndFeel": {
    "global": {
      "dragAndDrop": true,
      "indicators": true,
      "spinner": true,
      "spinnerColor": "accent",
      "spinnerDiameter": 50,
      "zoomToDataStrategy": "centroid"
    }
  },
  "timeline": {
    "global": {
      "useDetailedTimeline": true,
      "useUtc": true,
      "tabsContainer": {
        "dataStep": {
          "timeline": {
            "aggregation": {
              "aggregationBucketOrInterval": "bucket",
              "aggregationBucketsNumber": 50,
              "aggregationIntervalUnit": "hour",
              "aggregationIntervalSize": 1
            }
          },
          "detailedTimeline": {
            "bucketsNumber": 50
          }
        },
        "renderStep": {
          "timeline": {
            "chartTitle": "Timeline",
            "chartType": "bars",
            "dateFormat": "%b %d %Y  %H:%M",
            "isMultiselectable": true
          },
          "detailedTimeline": {
            "chartTitle": "Detailed timeline",
            "chartType": "bars",
            "dateFormat": "%b %d %Y  %H:%M",
            "selectionExtentPercent": 2
          }
        }
      },
      "unmanagedFields": {
        "dataStep": {
          "timeline": {
            "name": "Timeline",
            "icon": "watch_later",
            "isOneDimension": false
          },
          "detailedTimeline": {
            "name": "Timeline",
            "icon": "watch_later",
            "isOneDimension": false
          }
        },
        "renderStep": {
          "timeline": {
            "xTicks": 9,
            "yTicks": 2,
            "xLabels": 9,
            "yLabels": 2,
            "xUnit": "",
            "yUnit": "",
            "chartXLabel": "",
            "shortYLabels": false,
