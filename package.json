{
  "name": "arlas-wui-builder",
  "version": "27.2.0-dev",
  "scripts": {
    "ng": "ng",
    "postinstall": "patch-package --patch-dir ./patches && patch-package --patch-dir ./node_modules/arlas-web-core/patches && patch-package --patch-dir ./node_modules/arlas-d3/patches",
    "start": "ng serve",
    "start:iam": "ng serve --ssl --ssl-cert=$EXPLO_SSL_CERT --ssl-key=$EXPLO_SSL_KEY",
    "build": "ng build --configuration production --aot --base-href='$ARLAS_BUILDER_BASE_HREF/'",
    "build:stats": "ng build --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "license-check": "license-check-and-add check -f license-check-and-add.json",
    "license-add": "license-check-and-add add -f license-check-and-add.json",
    "analyze": "webpack-bundle-analyzer dist/ARLAS-wui-builder/stats-es2015.json",
    "i18n:init": "ngx-translate-extract --input ./src/app --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src/app --output ./src/assets/i18n/{en,fr}.json --format json"
  },
  "dependencies": {
    "@angular/animations": "^20.3.16",
    "@angular/cdk": "^20.2.14",
    "@angular/common": "^20.3.16",
    "@angular/compiler": "^20.3.16",
    "@angular/core": "^20.3.16",
    "@angular/forms": "^20.3.16",
    "@angular/material": "^20.2.14",
    "@angular/platform-browser": "^20.3.16",
    "@angular/platform-browser-dynamic": "^20.3.16",
    "@angular/router": "^20.3.16",
    "@gisaia-team/ngx-icon-picker": "20.0.0",
    "@mapbox/tilebelt": "^1.0.2",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "@types/file-saver": "^2.0.1",
    "ajv": "8.8.2",
    "ajv-keywords": "5.1.0",
    "ang-jsoneditor": "3.1.0",
    "arlas-wui-toolkit": "28.0.0-beta.2",
    "arlas-maplibre": "28.0.0-beta.3",
    "file-saver": "^2.0.2",
    "jsoneditor": "9.10.2",
    "moment": "^2.29.4",
    "ngx-color-picker": "^20.1.1",
    "ngx-logger": "^5.0.12",
    "ngx-mat-select-search": "^8.0.0",
    "ngx-spinner": "^17.0.0",
    "patch-package": "^6.4.7",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.14",
    "@angular-eslint/builder": "~20.7.0",
    "@angular-eslint/eslint-plugin": "~20.7.0",
    "@angular-eslint/eslint-plugin-template": "~20.7.0",
    "@angular-eslint/schematics": "~20.7.0",
    "@angular-eslint/template-parser": "~20.7.0",
    "@angular/cli": "^20.3.14",
    "@angular/compiler-cli": "^20.3.16",
    "@angular/language-service": "^20.3.16",
    "@colsen1991/ngx-translate-extract-marker": "^3.0.1",
    "@vendure/ngx-translate-extract": "^9.3.0",
    "@ngneat/spectator": "~9.0.0",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/jasmine": "~3.10.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "jasmine-core": "~3.10.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "license-check-and-add": "^4.0.5",
    "ng-mocks": "~14.13.1",
    "ng-packagr": "^20.3.2",
    "node": "^24.10.1",
    "patch-package": "^6.4.7",
    "ts-node": "~10.4.0",
    "typescript": "5.9.3",
    "webpack-bundle-analyzer": "^4.6.1"
  },
  "resolutions": {
    "@babel/preset-env": "7.5.5"
  }
}
