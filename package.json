{
  "name": "arlas-wui-builder",
  "version": "27.1.0-dev",
  "scripts": {
    "ng": "ng",
    "postinstall": "patch-package --patch-dir ./node_modules/arlas-web-core/patches && patch-package --patch-dir node_modules/arlas-d3/patches",
    "start": "ng serve",
    "build": "ng build --configuration production --aot --base-href='$ARLAS_BUILDER_BASE_HREF/'",
    "build:stats": "ng build --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "license-check": "license-check-and-add check -f license-check-and-add.json",
    "license-add": "license-check-and-add add -f license-check-and-add.json",
    "analyze": "webpack-bundle-analyzer dist/ARLAS-wui-builder/stats-es2015.json",
    "i18n:init": "ngx-translate-extract --input ./src/app --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src/app --output ./src/assets/i18n/{en,fr}.json --format json"
  },
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@gisaia-team/ngx-icon-picker": "18.0.1",
    "@mapbox/tilebelt": "^1.0.2",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@types/file-saver": "^2.0.1",
    "ajv": "8.8.2",
    "ajv-keywords": "5.1.0",
    "ang-jsoneditor": "3.1.0",
    "arlas-wui-toolkit": "~27.0.7",
    "arlas-maplibre": "~27.0.7",
    "file-saver": "^2.0.2",
    "jsoneditor": "9.10.2",
    "moment": "^2.29.4",
    "ngx-color-picker": "^17.0.0",
    "ngx-logger": "^5.0.12",
    "ngx-spinner": "^17.0.0",
    "patch-package": "^6.4.7",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zone.js": "^0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular-eslint/builder": "18.4.3",
    "@angular-eslint/eslint-plugin": "18.4.3",
    "@angular-eslint/eslint-plugin-template": "18.4.3",
    "@angular-eslint/schematics": "18.4.3",
    "@angular-eslint/template-parser": "18.4.3",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.13",
    "@angular/language-service": "^18.2.13",
    "@colsen1991/ngx-translate-extract-marker": "^3.0.1",
    "@vendure/ngx-translate-extract": "^9.3.0",
    "@ngneat/spectator": "~9.0.0",
    "@types/jasmine": "~3.10.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^18.19.1",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "jasmine-core": "~3.10.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "license-check-and-add": "^4.0.5",
    "ng-mocks": "~14.13.1",
    "ng-packagr": "^18.2.1",
    "node": "^18.19.1",
    "patch-package": "^6.4.7",
    "ts-node": "~10.4.0",
    "typescript": "5.5.4",
    "webpack-bundle-analyzer": "^4.6.1"
  },
  "resolutions": {
    "@babel/preset-env": "7.5.5"
  },
  "overrides": {
    "esbuild": "~0.25.1"
  }
}
