{
  "name": "arlas-wui-builder",
  "version": "21.0.0-beta.4",
  "scripts": {
    "ng": "ng",
    "postinstall": "patch-package --patch-dir ./node_modules/arlas-web-core/patches",
    "start": "ng serve",
    "build": "node --max_old_space_size=6144 node_modules/@angular/cli/bin/ng build --prod --aot --base-href='$ARLAS_BUILDER_BASE_HREF/'",
    "build:stats": "ng build --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "licence-check": "license-check-and-add check -f license-check-and-add.json",
    "licence-add": "license-check-and-add add -f license-check-and-add.json",
    "analyze": "webpack-bundle-analyzer dist/ARLAS-wui-builder/stats-es2015.json",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{en,fr}.json --format json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.0.2",
    "@angular/cdk": "13.0.2",
    "@angular/common": "13.0.2",
    "@angular/compiler": "13.0.2",
    "@angular/core": "13.0.2",
    "@angular/forms": "13.0.2",
    "@angular/material": "13.0.2",
    "@angular/platform-browser": "13.0.2",
    "@angular/platform-browser-dynamic": "13.0.2",
    "@angular/router": "13.0.2",
    "@mapbox/tilebelt": "^1.0.2",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "@types/file-saver": "^2.0.1",
    "ajv": "8.8.2",
    "ajv-keywords": "5.1.0",
    "arlas-wui-toolkit": "21.0.0-beta.3",
    "ang-jsoneditor": "1.10.5",
    "core-js": "~3.19.3",
    "d3-interpolate": "^1.4.0",
    "file-saver": "^2.0.2",
    "jsoneditor": "9.5.6",
    "moment": "^2.24.0",
    "ngx-color-picker": "11.0.0",
    "@gisaia-team/ngx-icon-picker": "1.8.0-beta.1",
    "ngx-logger": "4.2.1",
    "ngx-spinner": "^13.0.0",
    "patch-package": "^6.4.7",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.1",
    "@angular/cli": "~13.2.1",
    "@angular/compiler-cli": "13.0.2",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/language-service": "13.0.2",
    "@babel/runtime": "7.12.5",
    "@biesbjerg/ngx-translate-extract": "7.0.4",
    "@biesbjerg/ngx-translate-extract-marker": "1.0.0",
    "@ngneat/spectator": "~9.0.0",
    "@types/d3-interpolate": "^1.3.1",
    "@types/jasmine": "~3.10.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "16.11.20",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.6.0",
    "@typescript-eslint/parser": "5.3.0",
    "codelyzer": "~6.0.1",
    "eslint": "^8.2.0",
    "jasmine-core": "~3.10.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "license-check-and-add": "^3.0.4",
    "ng-mocks": "~12.5.1",
    "node": "16.13.2",
    "protractor": "~7.0.0",
    "run-parallel": "1.1.10",
    "ts-node": "~10.4.0",
    "typescript": "~4.4.3",
    "webpack-bundle-analyzer": "^3.6.1"
  },
  "resolutions": {
    "@babel/preset-env": "7.5.5"
  }
}
